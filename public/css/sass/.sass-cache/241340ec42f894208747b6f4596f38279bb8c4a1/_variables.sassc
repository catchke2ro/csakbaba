@mixin checkbox
  input
    display: none
    & + label
      position: relative
      cursor: pointer
      display: block
      margin: 5px 0px
      &:after
        content: ""
        display: block
        position: absolute
        top: -10px
        left: 100%
        background-color: rgba(lighten($col1, 20%), 0.8)
        height: 30px
        width: 300%
        background-repeat: no-repeat
        background-position: 10px center
        background-image: url(/img/elements/cb.png)
        -webkit-touch-callout: none
        -webkit-user-select: none
        -moz-user-select: none
        -ms-user-select: none
        user-select: none
    &:checked + label:after
      background-image: url(/img/elements/cb_active.png)

@mixin radio
  min-height: 30px
  height: auto
  padding-top: 15px
  top: auto
  +transform(none)
  input
    display: none
  & > label
    top: 0px
    left: 0px
    padding: 5px
    +transform(none)
  div.radios
    position: relative
    margin-top: 20px
    label
      cursor: pointer
      display: block
      position: relative
      margin: 20px 0px
      font-weight: normal
      &:after
        content: ""
        display: block
        position: absolute
        top: -10px
        left: 100%
        width: 300%
        height: 30px
        background-color: rgba(lighten($col1, 20%), 0.8)
        background-repeat: no-repeat
        background-position: 10px center
        background-image: url(/img/elements/radio.png)
        -webkit-touch-callout: none
        -webkit-user-select: none
        -moz-user-select: none
        -ms-user-select: none
        user-select: none
      &:first-child
        margin-top: 0px
      &:last-child
        margin-bottom: 0px
    label.checked:after
      background-image: url(/img/elements/radio_active.png)

@mixin multicheckbox
  min-height: 30px
  height: auto
  padding: 15px 0px 5px
  top: auto
  +transform(none)
  input
    display: none
  & > label
    top: 0px
    left: 0px
    padding: 5px
    +transform(none)
  div.radios
    position: relative
    margin-top: 20px
    label
      cursor: pointer
      display: block
      position: relative
      margin: 20px 0px
      font-weight: normal
      &:after
        content: ""
        display: block
        position: absolute
        top: -10px
        left: 100%
        width: 300%
        height: 30px
        background-color: rgba(lighten($col1, 20%), 0.8)
        background-repeat: no-repeat
        background-position: 10px center
        background-image: url(/img/elements/cb.png)
        -webkit-touch-callout: none
        -webkit-user-select: none
        -moz-user-select: none
        -ms-user-select: none
        user-select: none
      &:first-child
        margin-top: 0px
      &:last-child
        margin-bottom: 0px
    label.checked:after
      background-image: url(/img/elements/cb_active.png)





@mixin form
  ul
    width: 100%
  li.hidden
    display: none
  li.field
    height: 30px
    background-color: rgba(darken($col1, 40%), 0.4)
    width: 98%
    position: relative
    padding: 1%
    border-bottom: 1px rgba($col1,0.5)

    +clearFix
  label
    width: 25%
    color: lighten($col1, 80%)
    text-align: right
    padding-right: 10px
    +box-sizing(border-box)
    font-weight: bold
    display: block
    +valign
    position: absolute
  input, select, li.textarea > .cke, li.textarea textarea:not(.ck)
    border: medium none
    font-size: 1.6em
    padding: 5px
    background-color: rgba(lighten($col1, 20%), 0.8)
    color: lighten($col1, 80%)
    float: left
    margin-left: 25%
    width: 75%
    height: 100%
    +box-sizing(border-box)

  li.textarea > .cke
    padding: 0px

  li.textarea
    min-height: 30px
    height: auto
    textarea.ck
      display: none

  li.checkbox
    +checkbox

  li.radio
    +radio

  li.multicheckbox
    +multicheckbox

  li.field.upload
    height: auto
    min-height: 30px
    label
      display: none

  li.field.submit
    width: 20%
    min-width: 200px
    margin: auto
    padding: 10px
    background: transparent
    input
      cursor: pointer
      margin: 0px
      width: 100%
      background-color: $col2

  li.field.errors
    box-shadow: 0px 0px 3px red
    ul.errors
      position: absolute
      top: 0px
      right: 0px
      width: 100px
      height: 100%
      display: none
      background: none repeat scroll 0 0 rgba(255, 255, 255, 0.5)
      color: #FF0000
      padding: 5px
      +box-sizing(border-box)
  li.field.errors:hover
    ul.errors
      display: block


  li.field.captcha
    height: 60px
    img
      float: left
      left: 0px
      width: 25%
      +valign
      position: absolute
      +box-sizing(border-box)

    input
      height: 30px
      margin-top: 30px
    label
      height: 30px
      left: 0
      line-height: 30px
      margin-left: 25%
      top: 0
      text-align: left
      padding-left: 5px
      +transform(none)

  fieldset legend
    padding: 5px 10px
    +box-sizing(border-box)
    background-color: rgba($col2,0.85)
    color: #FFFFFF
    font-size: 1.3em
    display: block
    width: 100%
    font-weight: bold

  .cke_wysiwyg_frame, .cke_wysiwyg_div, .cke_inner, .cke
    background: transparent
    box-shadow: none !important
  .cke_top
    background: none
    border: none
    box-shadow: none !important
    background-color: rgba(darken($col1, 20%), 0.66)
  .cke_toolgroup
    border-radius: 0px
    border: none
    box-shadow: none
    background: none
    background-color: #FFFFFF



.fileUploadContainer
  padding: 10px
  .fileinput-button
    border: 3px dashed #FFFFFF
    span
      background-color: $col2
      color: #FFFFFF
      border: 1px solid #FFFFFF
      display: block
      margin: 10px auto
      padding: 10px
      text-align: center
      width: 200px
    input
      height: 100%
      width: 100%
    &.in
      background-color: #FFFFFF
    &.hover
      background: #FFFFFF
  .template-upload, .template-download
    display: none
  .files, .fileupload-progress
    .progress
      height: 10px
      width: 90%
      border: 1px solid #FFFFFF
      margin: auto
      +box-sizing(border-box)
      .progress-bar
        background-color: $col1
        height: 100%
        width: 0px
    .error
      color: red
      padding: 1px 10px
      font-weight: bold
    .template-upload, .template-download
      display: block
      background-color: rgba(lighten($col1, 20%), 0.8)
      margin: 5px 0px
      height: 50px
      position: relative
    .template-upload
      color: #FFFFFF
      .size
        margin: 5px 5px 10px 0px
        float: right
      .filename
        float: left
        margin: 5px 0px 10px 5px
        color: #FFFFFF
        font-weight: bold
      .error
        margin-top: 5px
        &:before
          clear: both
          content: ""
          display: block
          float: none
    .template-download
      .delete
        background-color: $col2
        color: #FFFFFF
        cursor: pointer
        font-size: 1.2em
        height: 17px
        padding: 0 5px
        right: 10px
        width: 8px
        line-height: 17px
        +valign
        position: absolute
      .previewImage
        float: left
        margin: 0 10px 0 0
        overflow: hidden
        height: 100%
      .filename
        line-height: 50px
        color: #FFFFFF
        font-weight: bold
      img
        height: 100%
        +clearFix




@mixin tab()
  width: 100%
  background-color: rgba($col1,0.35)
  border-top: 1px solid $col1
  border-bottom: 1px solid $col1
  +clearFix
  a
    margin-left: 5%
    color: black
    display: block
    padding: 1%
    float: left
    font-size: 1.3em
    border: 1px solid $col1
    border-bottom: none
    border-top: none
    background-color: rgba(white,0.35)
    text-align: center
    +transition(0.6s)
    &:hover
      color: white
      background-color: saturate($col1,5%)
      +transition(0.6s)
    &.active
      color: white
      background-color: $col1
    +mq3
      padding: 2%
      margin-left: 0%
    +mq4
      width: 100%
      border: none





