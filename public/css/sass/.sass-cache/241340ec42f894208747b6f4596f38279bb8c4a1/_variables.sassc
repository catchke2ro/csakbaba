$col1: #8b7b6a //barna
//$col1: #6d8b6a //zÃ¶ld
//$col1: #6a778b //kÃ©k
$col2: adjust_color($col1, $red:-111, $green: -54, $blue: +8)
$col3: #71a6db
$col4: #f59ab1
$bgcol: adjust_color(lighten($col1, 40%), $green: -11, $blue: -22)



$sidebarWidth: 260px
$anim: cubic-bezier(.13,.94,.62,1.25)

@mixin clearFix
  &:after
    content: ""
    display: block 
    clear: both
    float: none
@mixin clearFixBefore
  &:before
    content: ""
    display: block
    clear: both
    float: none

@mixin valign
  position: relative
  top: 50%
  +transform(translateY(-50%))

@mixin ratio1
  position: relative
  &:before
    content: ""
    display: block
    padding-top: 110%

@mixin longtext($lwidth)
  width: $lwidth
  white-space: nowrap
  overflow: hidden
  text-overflow: ellipsis

@mixin mq-1
  @media (min-width: 1600px)
    @content

@mixin mq0
  @media (max-width: 1180px)
    @content

@mixin mq1
  @media (max-width: 960px)
    @content

@mixin mq2
  @media (max-width: 800px)
    @content

@mixin mq3
  @media (max-width: 550px)
    @content

@mixin mq4
  @media (max-width: 400px)
    @content

@mixin mq($prop, $mq1: false, $mq2: false, $mq3: false, $mq4: false)
  @if $mq1
    +mq1
      #{$prop}: $mq1
  @if $mq2
    +mq2
      #{$prop}: $mq2
  @if $mq3
    +mq3
      #{$prop}: $mq3
  @if $mq4
    +mq4
      #{$prop}: $mq4


